<?xml version="1.0" encoding="UTF-8"?>

<odoo>
    <record id='rf_estados_form' model='ir.ui.view'>
        <field name="name">rf.estados</field>
        <field name="model">rf.estados</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name='codigo' string='Código de Estado'/>             
                            <field name='estado' string='Estados'/>   
                        </group>
                    </group>

                </sheet>
            </form>

        </field>
    </record>


    <record id='rf_estados_tree' model='ir.ui.view'>
        <field name="name">rf.estados</field>
        <field name="model">rf.estados</field>
        <field name="arch" type="xml">
            <tree>
                <field name='codigo' string='Código de Estado'/>             
                <field name='estado' string='Estados'/>             
            </tree>
        </field>
    </record>

   
</odoo>